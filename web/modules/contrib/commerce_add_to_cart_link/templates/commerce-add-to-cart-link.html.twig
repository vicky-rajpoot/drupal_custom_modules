{#
/**
 * @file
 *
 * Default template for add to cart link.
 *
 * Available variables:
 * - attributes: HTML attributes for the wrapper.
 * - url: The URL to the add to cart route.
 * - product_variation: The product variation entity.
 *
 * @ingroup themeable
 */
#}

  <a href="{{ url }}" {{ attributes }} rel="nofollow" data-variation="{{ product_variation.id }}">{{ 'Add to cart'|t }}</a>
